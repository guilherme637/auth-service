<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Domain\Entity\AuthenticationService" schema="public" table="authorization_serve">
        <id name="id" type="integer" column="nu_seq_authorization_serve">
            <generator strategy="AUTO"/>
        </id>

        <field name="clientId" type="string" column="client_id" nullable="false"/>
        <field name="redirectUri" type="string" column="redirect_uri" length="225" nullable="false"/>
        <field name="secret" type="string" column="secret" nullable="false"/>
        <field name="scopes" type="integer" column="nu_seq_scopes"/>
    </entity>
</doctrine-mapping>