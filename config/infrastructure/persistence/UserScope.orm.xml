<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Domain\Entity\UserScope" schema="public" table="users_scope">
        <id name="id" column="nu_seq_scope" type="integer">
            <generator strategy="AUTO"/>
        </id>

        <many-to-one field="users" target-entity="App\Domain\Entity\Users" inversed-by="scopes">
            <join-column name="nu_seq_users" referenced-column-name="nu_seq_users" />
        </many-to-one>
        <field name="dsScope" type="string" column="ds_scope" length="15"/>
    </entity>
</doctrine-mapping>